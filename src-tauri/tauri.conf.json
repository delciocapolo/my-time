{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "my-time",
  "version": "0.1.0",
  "identifier": "com.delciocapolo.my-time",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "my-time",
        "width": 350,
        "height": 320,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "plugins": {
    "sql": {
      "preload": ["sqlite:my-time-database.db"]
    },
    "updater": {
      "windows": {
        "installMode": "passive"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDY0NEI1NEYzQUExQjA1REIKUldUYkJSdXE4MVJMWk1qTi9mY0NkazZ0V1lqUitlMWtmeEpUK3k2bnRUYkRIY3FGWHlHL2RIbUMK",
      "endpoints": [
        "https://github.com/delciocapolo/my-time/releases/latest/download/latest.json"
      ]
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": ["app", "deb", "msi"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}