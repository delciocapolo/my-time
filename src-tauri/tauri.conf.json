{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "my-time",
  "version": "0.1.0",
  "identifier": "com.delciocapolo.my-time",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "my-time",
        "width": 350,
        "height": 320,
        "alwaysOnTop": true
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "plugins": {
    "sql": {
      "preload": ["sqlite:my-time-database.db"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}